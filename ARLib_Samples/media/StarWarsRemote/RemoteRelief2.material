    technique
    {
        pass RemoteRelief2
        {
            ambient 0.7572466135025024 0.7286241054534912 0.6946334838867188 1.0
            diffuse 0.605797299829085 0.5828992930486692 0.5557067953900514 1.0
            specular 0.5 0.5 0.5 1.0 12.5
